<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background" android:top="2.0px" android:bottom="2.0px">
        <shape>
            <gradient android:startColor="#ffbdc0c0" android:endColor="#ffbdc0c0" android:angle="270.0" />
        </shape>
    </item>
    <item android:id="@android:id/progress" android:top="4.0px" android:bottom="4.0px">
        <clip>
            <shape>
                <gradient android:startColor="#ff5eb7c8" android:endColor="#ff5eb7c8" android:angle="360.0" />
            </shape>
        </clip>
    </item>
</layer-list>